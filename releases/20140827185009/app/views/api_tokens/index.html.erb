<h1>Listing api_tokens</h1>

<table>
  <thead>
    <tr>
      <th>Access token</th>
      <th>Account</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @api_tokens.each do |api_token| %>
      <tr>
        <td><%= api_token.access_token %></td>
        <td><%= api_token.account_id %></td>
        <td><%= link_to 'Show', api_token %></td>
        <td><%= link_to 'Edit', edit_api_token_path(api_token) %></td>
        <td><%= link_to 'Destroy', api_token, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api token', new_api_token_path %>
